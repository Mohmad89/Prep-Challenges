            "source": [
                "function squares(a, b) {\r\n      let count = 0;\r\n      for(let i= a; i <= b ; i++){\r\n            if((Math.sqrt(i))%1 === 0 ){\r\n            count++;\r\n      }   \r\n      }\r\n      return count;\r\n}"
